#include<bits/stdc++.h>
using namespace std;
using ll=long long;

void solve()
{
	int n;
	cin>>n;
	ll arr[n];
	for (int i=0;i<n;i++) cin>>arr[i];
	for (int i=n-1;i>=0;i--) cout<<arr[i]<< ' ';
	cout<<endl;
}

int main()
{
    ios::sync_with_stdio(false); cin.tie(0); cout.tie(0);

    int t=1;
    cin>>t;
    
    while(t--) solve();
    // for (int i=1;i<=t;i++) {cout<<"Case "<<i<<": "; solve();}
}

